{% extends 'layout.njk' %}
{% block content %}
        <div class="container">
            <img src="https://pbs.twimg.com/profile_images/1271517147349626881/Mf1UjRa0.jpg" alt="Logo da Rocketseat">
            <h1 id="titulo">Rocketseat</h1>
            <h2 id="descricao">Conosco você irá aprender a fazer sites incríveis</h2>
            <ul id="lista">
                <li>Html</li>
                <li>CSS</li>
                <li>JavaScript</li>
            </ul>
        </div>
        <footer>
            <div class="links">
            {% for link in links %}
                    <a href="{{link.url}}">{{link.name}}</a>
            {% endfor %}
            </div>
        </footer>
{% endblock content %}
